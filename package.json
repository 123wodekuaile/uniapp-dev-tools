{
  "name": "anneng-uniapp-dev-tools",
  "version": "1.0.0",
  "description": "支持 Vue2+Vue3 的跨平台调试工具",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "anneng-uniapp-dev-tools": "./bin/anneng-uniapp-dev-tools"
  },
  "files": [
    "dist",
    "core",
    "page",
    "scripts",
    "bin"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch"
  },
  "keywords": [
    "uniapp",
    "vue",
    "debug",
    "devtools",
    "调试工具"
  ],
  "author": "chint",
  "license": "MIT",
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3"
  },
  "peerDependencies": {
    "vue": ">=2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chint/uniDevTools.git"
  }
} 